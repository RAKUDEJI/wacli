package example:hello-registry@0.1.0;

/// Registry world - imports commands, exports registry
world hello-registry {
  /// Import greeter command
  import greeter: interface {
    use fw:cli/types@1.0.0.{command-meta, command-result};
    meta: func() -> command-meta;
    run: func(argv: list<string>) -> command-result;
  }

  /// Export registry for core to use
  export fw:cli/registry@1.0.0;
}
