MOLT_REGISTRY="https://molt-registry-production.sdweb.workers.dev"

# Auth (choose one)
#
# 1) Basic auth (wacli will derive Authorization header automatically)
USERNAME="your-username"
PASSWORD="your-password"
#
# 2) Or provide an explicit header (overrides USERNAME/PASSWORD)
# MOLT_AUTH_HEADER="Authorization: Bearer <token>"
# MOLT_AUTH_HEADER="Authorization: Basic <base64(username:password)>"

# Optional: override framework repos/refs
# WACLI_HOST_REPO="wacli/host"
# WACLI_CORE_REPO="wacli/core"
# WACLI_HOST_REFERENCE="v0.0.40"
# WACLI_CORE_REFERENCE="v0.0.40"

# Optional: force re-pull of registry plugins (build.commands)
# WACLI_REGISTRY_REFRESH=1
